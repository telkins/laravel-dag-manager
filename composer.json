{
    "name": "telkins/laravel-dag-manager",
    "description": "A SQL-based Directed Acyclic Graph (DAG) solution for Laravel.",
    "keywords": [
        "telkins",
        "laravel-dag-manager",
        "sql",
        "dag",
        "directed",
        "acyclic",
        "graph",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-data": "^4.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "Telkins\\Dag\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Telkins\\Dag\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Telkins\\Dag\\Providers\\DagServiceProvider"
            ]
        }
    }
}